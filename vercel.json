{
  "crons": [
    {
      "path": "/api/cron/ingest?source=adzuna",
      "schedule": "0 */4 * * *"
    },
    {
      "path": "/api/cron/ingest?source=jooble",
      "schedule": "0 1,7,13,19 * * *"
    },
    {
      "path": "/api/cron/freshness-decay",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/ingest?source=greenhouse",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/send-alerts",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/expiry-warnings",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/cleanup-descriptions",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*\\.(png|jpg|jpeg|gif|webp|svg|ico|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
