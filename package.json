{
  "name": "pmhnp-job-board",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf .next out dist",
    "validate": "npm run type-check && npm run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "postinstall": "prisma generate",
    "audit:jobs": "ts-node --project scripts/tsconfig.json scripts/audit-and-fix-jobs.ts",
    "audit:salaries": "ts-node --project scripts/tsconfig.json scripts/audit-salaries.ts",
    "fix:locations": "ts-node --project scripts/tsconfig.json scripts/fix-missing-locations.ts",
    "fix:salaries": "ts-node --project scripts/tsconfig.json scripts/fix-all-salaries.ts",
    "populate:display-salaries": "ts-node --project scripts/tsconfig.json scripts/populate-display-salaries.ts",
    "clean:descriptions": "ts-node --project scripts/tsconfig.json scripts/clean-job-descriptions.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.88.0",
    "@tailwindcss/typography": "^0.5.19",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/og": "^0.8.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.555.0",
    "next": "^16.0.10",
    "next-mdx-remote": "^5.0.0",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.67.0",
    "resend": "^6.5.2",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "prisma": "^7.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
